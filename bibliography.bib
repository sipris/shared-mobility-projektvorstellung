@inproceedings{abu-matar2016smart-city,
  title = {Towards a Software Defined Reference Architecture for Smart City Ecosystems},
  booktitle = {2016 {{IEEE International Smart Cities Conference}} ({{ISC2}})},
  author = {Abu-Matar, Mohammad},
